package com.higor.app.dsvendas.dto;

import com.higor.app.dsvendas.entities.Seller;

public class SaleSumDTO {

	private String sellerName;
	private Double sum;

	public SaleSumDTO() {
	}

	public SaleSumDTO(Seller seller, Double sum) {
		this.sellerName = seller.getName();
		this.sum = sum;
	}
	
	

}
